	-- Scripts:

	local function LPPI_fake_script() -- TextButton.LocalScript 
		local script = Instance.new('LocalScript', TextButton)

		script.Parent.MouseButton1Click:Connect(function()
			game.Players.LocalPlayer.PlayerGui.ScreenGui.Frame["Scripts2"].Visible = false
			game.Players.LocalPlayer.PlayerGui.ScreenGui.Frame["Coming Soon"].Visible = false
			game.Players.LocalPlayer.PlayerGui.ScreenGui.Frame["Scripts"].Visible = true
		end)
	end
	coroutine.wrap(LPPI_fake_script)()
	local function CEXEWY_fake_script() -- TextButton_2.LocalScript 
		local script = Instance.new('LocalScript', TextButton_2)

		script.Parent.MouseButton1Click:Connect(function()
			game.Players.LocalPlayer.PlayerGui.ScreenGui.Frame["Scripts2"].Visible = true
			game.Players.LocalPlayer.PlayerGui.ScreenGui.Frame["Coming Soon"].Visible = false
			game.Players.LocalPlayer.PlayerGui.ScreenGui.Frame["Scripts"].Visible = false
		end)
	end
	coroutine.wrap(CEXEWY_fake_script)()
	local function YHAZOK_fake_script() -- Script.LocalScript 
		local script = Instance.new('LocalScript', Script)

		script.Parent.MouseButton1Click:Connect(function()

	--[[
	Fedoratum FE Explode 4nn1 Place
	
	Click explode button to explode
	Rejoin to stop script
	To kill other people make sure you are really close when you explode
	
	You can combine with any fly.
	]]

			-- // Netless to be able to modify NPC health
			-- // Netless by gelatek

			local Workspace = game:GetService("Workspace")
			local Client = game:GetService("Players").LocalPlayer
			local Character = Client.Character
			local Humanoid = Character:FindFirstChildOfClass("Humanoid")
			local Physics = settings()["Physics"]

			Workspace.InterpolationThrottling = Enum.InterpolationThrottlingMode.Disabled
			Workspace.Retargeting = "Disabled"
			Client.ReplicationFocus = Workspace

			pcall(function()
				sethiddenproperty(Humanoid, "InternalBodyScale", Vector3(9e9,9e9,9e9))
				sethiddenproperty(Workspace, "SignalBehavior", "Immediate")
				Physics.PhysicsEnvironmentalThrottle = Enum.EnviromentalPhysicsThrottle.Disabled
				Physics.AllowSleep = false
				Physics.ThrottleAdjustTime = math.huge
			end)

			local RunService = game:GetService("RunService")

			SuperStepper = Instance.new("BindableEvent")
			setfflag("NewRunServiceSignals","true")
			for _,vQ in pairs({RunService.RenderStepped,RunService.Heartbeat,RunService.Stepped,RunService.PreSimulation,RunService.PostSimulation}) do
				vQ:Connect(function()
					return
						SuperStepper:Fire(SuperStepper,tick())
				end)
			end

			SuperStepper.Event:Connect(function()
				if Character and Humanoid then
					sethiddenproperty(Humanoid, "InternalBodyScale", Vector3.new(9e9,9e9,9e9))
				end
			end)

			Client.SimulationRadiusChanged:Connect(function()
				Radius = 1e+10
				return Radius
			end)

			SuperStepper.Event:Connect(function()
				sethiddenproperty(Client, "MaximumSimulationRadius", 1e+10)
				sethiddenproperty(Client, "SimulationRadius", 1e+10)
			end)

			-- // Check network owner

			local isnetworkowner = isnetworkowner or function(Part)
				return Part.ReceiveAge == 0
			end

			-- // GUI

			local ScreenGui = Instance.new("ScreenGui")
			local KaboomButton = Instance.new("TextButton")

			ScreenGui.Parent = game:GetService("CoreGui")
			ScreenGui.ResetOnSpawn = false

			KaboomButton.Name = "SilentCMD"
			KaboomButton.Parent = ScreenGui
			KaboomButton.BackgroundColor3 = Color3.new(0.168627, 0.513726, 0.25098)
			KaboomButton.BorderColor3 = Color3.new(0.168627, 0.513726, 0.25098)
			KaboomButton.Position = UDim2.new(0.912970064, 0, 0.194202876, 0)
			KaboomButton.Size = UDim2.new(0, 50, 0, 50)
			KaboomButton.Font = Enum.Font.Gotham
			KaboomButton.Text = "Explode"
			KaboomButton.TextColor3 = Color3.new(1, 1, 1)
			KaboomButton.TextSize = 14
			KaboomButton.TextStrokeColor3 = Color3.new(1, 1, 1)
			KaboomButton.TextWrapped = true
			KaboomButton.Transparency = 0.2

			-- // NPC that explodes

			local cats = Workspace.socky
			local char = Client.Character

			for i, x in pairs(cats:GetDescendants()) do
				if x:IsA("BasePart") then
					x.CanCollide = false
				end
			end

			Workspace.DescendantAdded:Connect(function(cat)
				if cat.Name == "socky" then
					task.wait(0.1)
					cats = Workspace.socky

					for i, x in pairs(cats:GetDescendants()) do
						if x:IsA("BasePart") then
							x.CanCollide = false
						end
					end

				end
			end)

			-- // Check NPC netship to ensure its right

			task.spawn(function()
				pcall(function()
					while task.wait() do
						if char and char.Torso and char:FindFirstChildOfClass("Humanoid") and char.Humanoid.RootPart and cats.Torso and cats then
							if isnetworkowner(cats.Torso) then
								cats:FindFirstChild("Torso").CFrame = char:FindFirstChild("Torso").CFrame
							else
								char:FindFirstChildOfClass("Humanoid").RootPart.CFrame = cats:FindFirstChild("Torso").CFrame
							end
						end
					end
				end)
			end)

			Client.CharacterAdded:Connect(function(chr)
				while task.wait() do
					pcall(function()
						if chr and chr.Torso and chr:FindFirstChildOfClass("Humanoid") and chr.Humanoid.RootPart and cats.Torso and cats then
							if isnetworkowner(cats:FindFirstChild("Torso")) then
								cats:FindFirstChild("Torso").CFrame = chr:FindFirstChild("Torso").CFrame
							else
								chr:FindFirstChildOfClass("Humanoid").RootPart.CFrame = cats:FindFirstChild("Torso").CFrame
							end
						end
					end)
				end
			end)

			-- // Kill to make npc explode
			-- // explosion has blastradius
			-- // therefore it kills other people

			KaboomButton.MouseButton1Click:Connect(function()
				pcall(function()
					if cats and cats.Humanoid and cats.Torso then
						if isnetworkowner(cats.Torso) then
							cats.Humanoid.Health = 0
						end
					end
				end)
			end)
		end)
	end
	coroutine.wrap(YHAZOK_fake_script)()
	local function DLFTC_fake_script() -- Script_2.LocalScript 
		local script = Instance.new('LocalScript', Script_2)

		script.Parent.MouseButton1Click:Connect(function()
			loadstring(game:HttpGet(('https://pastebin.com/raw/9sxf7TVz'),true))()
		end)
	end
	coroutine.wrap(DLFTC_fake_script)()

			KaboomButton.Name = "SilentCMD"
			KaboomButton.Parent = ScreenGui
			KaboomButton.BackgroundColor3 = Color3.new(0.168627, 0.513726, 0.25098)
			KaboomButton.BorderColor3 = Color3.new(0.168627, 0.513726, 0.25098)
			KaboomButton.Position = UDim2.new(0.912970064, 0, 0.194202876, 0)
			KaboomButton.Size = UDim2.new(0, 50, 0, 50)
			KaboomButton.Font = Enum.Font.Gotham
			KaboomButton.Text = "Explode"
			KaboomButton.TextColor3 = Color3.new(1, 1, 1)
			KaboomButton.TextSize = 14
			KaboomButton.TextStrokeColor3 = Color3.new(1, 1, 1)
			KaboomButton.TextWrapped = true
			KaboomButton.Transparency = 0.2

			-- // NPC that explodes

			local cats = Workspace.muffy
			local char = Client.Character

			for i, x in pairs(cats:GetDescendants()) do
				if x:IsA("BasePart") then
					x.CanCollide = false
				end
			end

			Workspace.DescendantAdded:Connect(function(cat)
				if cat.Name == "muffy" then
					task.wait(0.1)
					cats = Workspace.muffy

					for i, x in pairs(cats:GetDescendants()) do
						if x:IsA("BasePart") then
							x.CanCollide = false
						end
					end

				end
			end)

			-- // Check NPC netship to ensure its right

			task.spawn(function()
				pcall(function()
					while task.wait() do
						if char and char.Torso and char:FindFirstChildOfClass("Humanoid") and char.Humanoid.RootPart and cats.Torso and cats then
							if isnetworkowner(cats.Torso) then
								cats:FindFirstChild("Torso").CFrame = char:FindFirstChild("Torso").CFrame
							else
								char:FindFirstChildOfClass("Humanoid").RootPart.CFrame = cats:FindFirstChild("Torso").CFrame
							end
						end
					end
				end)
			end)

			Client.CharacterAdded:Connect(function(chr)
				while task.wait() do
					pcall(function()
						if chr and chr.Torso and chr:FindFirstChildOfClass("Humanoid") and chr.Humanoid.RootPart and cats.Torso and cats then
							if isnetworkowner(cats:FindFirstChild("Torso")) then
								cats:FindFirstChild("Torso").CFrame = chr:FindFirstChild("Torso").CFrame
							else
								chr:FindFirstChildOfClass("Humanoid").RootPart.CFrame = cats:FindFirstChild("Torso").CFrame
							end
						end
					end)
				end
			end)

			-- // Kill to make npc explode
			-- // explosion has blastradius
			-- // therefore it kills other people

			KaboomButton.MouseButton1Click:Connect(function()
				pcall(function()
					if cats and cats.Humanoid and cats.Torso then
						if isnetworkowner(cats.Torso) then
							cats.Humanoid.Health = 0
						end
					end
				end)
			end)
		end)
	end
	coroutine.wrap(YQCOR_fake_script)()
	local function FCITX_fake_script() -- Frame.Dragify 
		local script = Instance.new('LocalScript', Frame)

		local UIS = game:GetService("UserInputService")
		function dragify(Frame)
			dragToggle = nil
			local dragSpeed = 0
			dragInput = nil
			dragStart = nil
			local dragPos = nil
			function updateInput(input)
				local Delta = input.Position - dragStart
				local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
				game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.25), {Position = Position}):Play()
			end
			Frame.InputBegan:Connect(function(input)
				if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
					dragToggle = true
					dragStart = input.Position
					startPos = Frame.Position
					input.Changed:Connect(function()
						if input.UserInputState == Enum.UserInputState.End then
							dragToggle = false
						end
					end)
				end
			end)
			Frame.InputChanged:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
					dragInput = input
				end
			end)
			game:GetService("UserInputService").InputChanged:Connect(function(input)
				if input == dragInput and dragToggle then
					updateInput(input)
				end
			end)
		end

		dragify(script.Parent)
	end
	coroutine.wrap(FCITX_fake_script)()

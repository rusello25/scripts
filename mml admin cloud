setCommand("message",function(args)
	if game.PlaceId ~= 662417684 then return end
	local plr = game:GetService("Players").LocalPlayer
	local chr = plr.Character
	function chr() return plr.Character end
	function c() return plr.Backpack:FindFirstChild("PompousTheCloud") or chr():FindFirstChild("PompousTheCloud") end
	function sc() return c().ServerControl end
	function fixasset(id) return string.format("rbxthumb://type=Asset&id=%d&w=420&h=420",id) end
	function setproperty(object,property,value)
		sc():InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function sp(obj,p,v)
		task.spawn(setproperty,obj,p,v)
	end
	function spp(obj,p,v)
		c().Parent = chr()
		sp(obj,p,v)
		c().Parent = plr.Backpack
	end
	function fly(flying)
		return sc():InvokeServer("Fly",{Flying=flying})
	end

	local msg = table.concat(args," ")
	local hab = plr.Backpack:FindFirstChild("HeartAirBalloon") or chr():FindFirstChild("HeartAirBalloon")
	local gui = hab.Script.Gui
	hab.Parent = chr()
	spp(gui.Frame.Message,"Text",msg)
	spp(gui.Frame.Message,"TextColor3",Color3.fromRGB(255,255,255))
	spp(gui.Frame.Message,"Position",UDim2.fromScale(0,0))
	spp(gui.Frame.Message,"Size",UDim2.fromScale(1,1))  
	spp(gui.Frame.Message,"TextSize",36)
	spp(gui.Frame,"Size",UDim2.fromScale(0.75,0.2))
	spp(gui.Frame,"Position",UDim2.fromScale(0.125,0))
	spp(gui.Frame,"Style",Enum.FrameStyle.DropShadow)
	spp(gui,"Parent",game.StarterGui)
	hab.Parent = plr.Backpack
	game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId, plr)
end,"[text] - puts a message onscreen")
setCommand("crash", function(args)
	local a = cloud()
	function setProp(object,prop,val)
		a["ServerControl"]:InvokeServer("SetProperty",{Value=val,Property=prop,Object=object})
	end
	setProp(a.Handle,"Size",Vector3.new(math.huge,math.huge,math.huge))
	setProp(a.Handle.Mesh,"Scale",Vector3.new(math.huge,math.huge,math.huge))
	a.ServerControl:InvokeServer("Fly",{Flying=true})
end,"[server] - crashing the server can take time")

setCommand("bighead", function(args)
	local chr = lplr.Character
	local targets = getPlayer(args[1])
	for _,target in pairs(targets) do
		local c = cloud()
		wait(0.07)
		c.ServerControl:InvokeServer("Fly",{Flying=true})
		local a = c:WaitForChild("EffectCloud")
		wait()
		c.ServerControl:InvokeServer("SetProperty",{Object=a.Mesh,Property="Scale",Value=Vector3.new(1.25,2.5,2.5)})
		c.ServerControl:InvokeServer("SetProperty",{Object=a.Mesh,Property="MeshType",Value=Enum.MeshType.Brick})
		c.ServerControl:InvokeServer("SetProperty",{Object=a.Mesh,Property="Parent",Value=target.Character.Head})
		c.ServerControl:InvokeServer("Fly",{Flying=false})
	end
end, "[plrs] - gives [plrs] big head.")


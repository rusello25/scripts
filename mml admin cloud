		lplr.Character:PivotTo(target.Character:GetPivot())
	end
end, "[plr] - Teleport to [plr]")
setCommand("noclip", function() 
	noclipC = runservice.Stepped:Connect(function()
		if lplr.Character then
			for i,v in pairs(lplr.Character:GetDescendants()) do
				if hasprop(v,"CanCollide") and v.CanCollide == true then
					v.CanCollide = false
					print(v)
				end
			end
		end
	end)
end, "- Self explainatory")
setCommand("clip", function()
	if noclipC then
		noclipC:Disconnect()
		noclipC = nil
	end
end, "- Disables noclip")

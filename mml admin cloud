setCommand("2d", function(args) 
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function sp(obj,p,v)
		task.spawn(setproperty,obj,p,v)
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	function s(i,p,v)
		c.Parent = chr
		sp(i,p,v)
		c.Parent = plr.Backpack
	end
	function r(tbl)
		return tbl[math.random(1,#tbl)]
	end
	for _,x in pairs(chr:GetDescendants()) do if x:isA"BasePart" then s(x,"Transparency",1) end end
	pcall(function() chr.Head.Mesh:Destroy() end)
	s(chr.Head,"Size",Vector3.new(8,8,0.1))
	s(chr.Head.face,"Texture","rbxassetid://"..args[1])
end, "[2d] - Changes the look of you to the id you enter.")


setCommand("nameurself", function(args) 
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	c.Parent = chr
	setproperty(chr.Humanoid,'DisplayName',args[1])
	c.Parent = plr.Backpack
end, "[plr] - gives you a new name")



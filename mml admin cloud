setCommand("grab", function(args)
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	local target = chr
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	c.Grip = CFrame.new(0,1,0.65) * CFrame.Angles(math.rad(0),math.rad(0),math.rad(0))
	c.Parent = chr
	c.LocalScript.Disabled = true
	wait()
	wait()
	setproperty(c.Handle.Mesh,"MeshId","rbxassetid://1")
	setproperty(c.Handle.Mesh,"TextureId","rbxassetid://1")
	setproperty(c.Handle.Mesh,"Scale",Vector3.new(3.25,3.25,3.25))
	function weld(part0,part1,c0,c1,selfparent)
		fly(true)
		local ec = c:WaitForChild("EffectCloud")
		local rootjoint = ec:WaitForChild("Weld")
		wait()
		setproperty(rootjoint,"Parent",not selfparent and part1 or chr.Torso)
		setproperty(rootjoint,"Part0",part0)
		setproperty(rootjoint,"Part1",part1 or chr.Torso)
		setproperty(rootjoint,"C0",c0 or CFrame.new(-5,5,0))
		setproperty(rootjoint,"C1",c1 or CFrame.new(-0.5,-0.3,0))
		return rootjoint
	end
	local anim = Instance.new("Animation")
	anim.AnimationId = "rbxassetid://27432686"
	local anim = chr.Humanoid:LoadAnimation(anim)
	anim.Looped = false
	local holding
	c.Activated:connect(function() if not holding then anim:Play() else repeat wait() until type(holding) ~= "number";  end end)
	c.Handle.Touched:connect(function(part)
		if not holding and anim.IsPlaying then
			if part.Parent:FindFirstChild("HumanoidRootPart") then
				anim.TimePosition = 0.5
				anim:AdjustSpeed(0)
				holding = true -- cuz weld yields
				holding = weld(part.Parent.HumanoidRootPart,chr["Right Arm"],CFrame.new(-0.5,0.5,0)*CFrame.Angles(math.rad(90),0,0))
			end
		end
	end)
end, "[plr] - grab ppl")

setCommand("banhammer", function(args)

	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	c.Parent = chr
--[[
if chr:FindFirstChild("WatermelonHat") then
    local k = chr.WatermelonHat.Handle.VFX_Att.watermelon
    setproperty(k,"Parent",c.Handle)
    setproperty(k,"Texture","rbxassetid://431559364")
    setproperty(k,"Rate",25)
    setproperty(k,"EmissionDirection",5)
    setproperty(k,"LightEmission",5)
    setproperty(k,"VelocityInheritance",0.5)
    setproperty(k,"Lifetime",NumberRange.new(2,4))
    setproperty(k,"Size",NumberRange.new(2,4))
end
]]
	local t = false
	setproperty(c.LocalScript,"Disabled",true)
	setproperty(c,"CanBeDropped",true)
	setproperty(c,"Name","BanHammer")
	setproperty(c,"TextureId","rbxassetid://430355036")
	setproperty(c.Handle.Mesh,"MeshId","http://www.roblox.com/asset/?id=10604848")
	setproperty(c.Handle.Mesh,"TextureId","http://www.roblox.com/asset/?id=10605252")
	setproperty(c.Handle.Mesh,"Scale",Vector3.new(0.625,0.625,0.625))
	setproperty(c.Handle,"Size",Vector3.new(4,8,4))
	setproperty(c,"Grip",CFrame.new(0,-2,0))
	setproperty(c.CoastingPose,"AnimationId","http://www.roblox.com/twohandswordwithswingout-item?id=32659706")
	c.Activated:Connect(function()
		c.Parent.Humanoid:LoadAnimation(c.CoastingPose):Play()
	end)
	c.Handle.Touched:connect(function(op)
		if not t and op.Parent:FindFirstChild"Torso" then
			t = true
			wait()
			wait()
			fly(true)
			local ec = c:WaitForChild("EffectCloud")
			setproperty(ec,"Transparency",1)
			setproperty(c.Handle,"Transparency",0)
			setproperty(ec.Weld,"Part1",op.Parent.Torso)
			setproperty(ec.Weld,"Part0",ec)
			setproperty(ec.Weld,"C0",CFrame.new(600,0,600))
			wait(0.25)
			fly(false)
			t = false
		end
	end)
end, "[plr] - gives you a ban hammer")

setCommand("icehammer", function(args) 
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	c.Parent = chr
	if chr:FindFirstChild("WatermelonHat") then
		local k = chr.WatermelonHat.Handle.VFX_Att.watermelon
		setproperty(k,"Parent",c.Handle)
		setproperty(k,"Texture","rbxassetid://431559364")
		setproperty(k,"Rate",25)
		setproperty(k,"EmissionDirection",2)
		setproperty(k,"LightEmission",5)
		setproperty(k,"VelocityInheritance",0.5)
		setproperty(k,"Lifetime",NumberRange.new(2,4))
		setproperty(k,"Size",NumberRange.new(2,4))
	end
	local t = false
	c.LocalScript.Disabled = true
	setproperty(c.Handle.Mesh,"MeshId","http://www.roblox.com/asset/?id=15924243")
	setproperty(c.Handle.Mesh,"TextureId","http://www.roblox.com/asset/?id=15924246")
	setproperty(c.Handle.Mesh,"Scale",Vector3.new(1.5,1.5,1.5))
	setproperty(c.Handle,"Size",Vector3.new(4,8,4))
	setproperty(c,"Grip",CFrame.new(0,0,-2)*CFrame.Angles(0,math.rad(90),math.rad(90)))
	setproperty(c.CoastingPose,"AnimationId","http://www.roblox.com/twohandswordwithswingout-item?id=32659706")
	c.Activated:Connect(function()
		c.Parent.Humanoid:LoadAnimation(c.CoastingPose):Play()
	end)
	c.Handle.Touched:connect(function(op)
		if not t and op.Parent:FindFirstChild"Torso" then
			t = true
			wait()
			wait()
			fly(true)
			local ec = c:WaitForChild("EffectCloud")
			local oldcf = op.Parent.Torso.CFrame
			setproperty(ec.Mesh,"MeshId","rbxassetid://2784482070")
			setproperty(ec.Mesh,"TextureId","")
			setproperty(ec,"Transparency",0.5)
			setproperty(ec.Weld,"C0",CFrame.new(0,0,0))
			setproperty(ec.Weld,"Part1",op.Parent.Torso)
			setproperty(ec.Weld,"Part0",ec)
			setproperty(ec,"Anchored",true)
			setproperty(ec,"CFrame",oldcf)
			setproperty(c.Handle,"Transparency",0)
			setproperty(c.Handle,"CanCollide",true)
			wait(3)
			setproperty(ec,"Anchored",false)
			setproperty(ec.Weld,"C0",CFrame.new(5000,5000,5000))
			t = false
		end
	end)
end, "[plr] - gives you an ice hammer")

setCommand("statue", function(args) 
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function spx2(obj,prop,val)
		task.spawn(setproperty,obj,prop,val)
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	c.LocalScript.Disabled = true
	c.Parent = chr
	if chr:FindFirstChild("GingerbreadPal") then
		local k = chr.GingerbreadPal.Handle.VFX_Att.peppermint
		setproperty(k,"Parent",c.Handle)
		setproperty(k,"Texture","rbxassetid://2770710438")
		setproperty(k,"Rate",25)
		setproperty(k,"EmissionDirection",-2)
		setproperty(k,"LightEmission",5)
		setproperty(k,"VelocityInheritance",0.5)
		setproperty(k,"Lifetime",NumberRange.new(2,4))
		setproperty(k,"Size",NumberRange.new(2,4))
	end
	wait()
	spx2(c,"CanBeDropped",true)
	spx2(c,"Name","Statue")
	spx2(c,"TextureId","rbxassetid://2770710438")
	spx2(c.Handle.Mesh,"MeshId",'rbxassetid://2770710422')
	spx2(c.Handle.Mesh,"TextureId",'rbxassetid://2770710438')
	spx2(c.Handle.Mesh,"Scale",Vector3.new(4,4,4))
	spx2(c.CoastingPose,"AnimationId","rbxassetid://218504594")
	c.Activated:Connect(function()
		wait(0.2)
		c.Parent.Humanoid:LoadAnimation(c.CoastingPose):Play()
	end)
	c.Handle.Touched:connect(function(op)
		if not t and op.Parent:FindFirstChild"Torso" then
			t = true
			wait()
			wait()
			fly(true)
			local ec = c:WaitForChild("EffectCloud")
			local oldcf = op.Parent.Torso.CFrame
			spx2(ec.Mesh,"MeshId","rbxassetid://2784482070")
			spx2(ec.Mesh,"TextureId","2770710438")
			spx2(ec.Mesh,"Scale",Vector3.new(1,1,1))
			spx2(ec,"Transparency",0.5)
			spx2(ec.Weld,'C0',CFrame.new(0,0,0))
			spx2(ec.Weld,'Part1',op.Parent.Torso)
			setproperty(ec.Weld,"Part0",ec)
			setproperty(ec,"Anchored",true)
			setproperty(ec,"CFrame",oldcf)
			spx2(c.Handle,"Transparency",0)
			spx2(c.Handle,"CanCollide",true)
			wait(3)
			spx2(ec,"Anchored",false)
			spx2(ec.Weld,"C0",CFrame.new(5000,5000,5000))
			t = false
		end
	end)

end, "[plr] - gives you a statue")


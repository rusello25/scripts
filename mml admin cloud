setCommand("getcloud",function()
	repeat wait() until game.IsLoaded
	if game.PlaceId == 662417684 then
		repeat wait() until pcall(function()
			return game.Players.LocalPlayer.Character
		end)

		repeat wait() until pcall(function()
			return game.Players.LocalPlayer.Backpack
		end)

		wait(0.25)
		local p = game:GetService("Players").LocalPlayer
		local b = p.Backpack
		repeat
			game:GetService("ReplicatedStorage").SpawnSuperBlock:FireServer() task.wait() until
		b:FindFirstChild("HeartAirBalloon") and b:FindFirstChild("PompousTheCloud")
		local heart = b.HeartAirBalloon
		local ptz = b.PompousTheCloud
		ptz.Parent = p.Character
		heart.Parent = p.Character
		ptz.ServerControl:InvokeServer("SetProperty", {Object=heart,Property="Parent",Value=p.StarterGear})
		task.wait()
		ptz.ServerControl:InvokeServer("SetProperty", {Object=ptz,Property="Parent",Value=p.StarterGear})
		p.Character:BreakJoints()
	end
end, "- Gives you a cloud")

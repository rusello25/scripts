setCommand("fastrape", function(args)
	local chr = lplr.Character
	local target = getPlayer(args[1])[1]
	local c = cloud()
	wait(0.07)
	c.ServerControl:InvokeServer("Fly",{Flying=true})
	local zaza = c:WaitForChild("EffectCloud"):WaitForChild("Weld")
	chr.Animate.Disabled = true
	for i,ZAZA in next, chr.Humanoid:GetPlayingAnimationTracks() do
		ZAZA:stop()
	end
	wait()
	c.ServerControl:InvokeServer("SetProperty",{Object=zaza,Property="Parent",Value=chr.HumanoidRootPart})
	c.ServerControl:InvokeServer("SetProperty",{Object=chr.Torso["Left Shoulder"],Property="C0",Value=CFrame.new(-1,0.5,-1)*CFrame.Angles(math.rad(45),0,math.rad(22.5))})
	c.ServerControl:InvokeServer("SetProperty",{Object=chr.Torso["Right Shoulder"],Property="C0",Value=CFrame.new(1,0.5,-1)*CFrame.Angles(math.rad(45),0,-math.rad(22.5))})
	c.ServerControl:InvokeServer("SetProperty",{Object=zaza,Property="C0",Value=CFrame.new(0,-1.5,-2)*CFrame.Angles(math.rad(90),math.rad(180),math.rad(180))})
	c.ServerControl:InvokeServer("SetProperty",{Object=zaza,Property="C1",Value=CFrame.new(0,0,0)})
	c.ServerControl:InvokeServer("SetProperty",{Object=zaza,Property="Part1",Value=target.Character.HumanoidRootPart})
	task.spawn(function()
		for i = 0,math.huge,0.5 do task.wait(0.5)
			c.ServerControl:InvokeServer("SetProperty",{Object=zaza,Property="C0",Value=CFrame.new(0,-1.5,-(2.5+(math.floor(i)==i and 0.5 or 0)))*CFrame.Angles(math.rad(90),math.rad(180),math.rad(180))})
		end
	end)
end, "[plr] - Does [plr]")

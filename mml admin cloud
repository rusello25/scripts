		task.spawn(setproperty,obj,prop,val)
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	c.Parent = chr
	wait()
	local oldcframe = chr.HumanoidRootPart.CFrame
	pcall(function()
		for i,mad in pairs(targets) do
			if c.Parent == chr then
				wait(0.1)
				fly(true)
				local ec = c:WaitForChild("EffectCloud")
				local joint = ec:WaitForChild("Weld")
				wait(0.2)
				spx2(joint,'Parent',chr)
				chr.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
				spx2(joint,'Part0',mad.Character.HumanoidRootPart)
				spx2(ec,'CFrame',CFrame.new() * chr.Torso.CFrame+Vector3.new(0,-600,0))
			end

		end
		wait(0.1)
		chr.Humanoid:UnequipTools()
		wait(0.1)
		if args[1] == "me" or "all" then repeat task.wait() until targets == plr chr.HumanoidRootPart.CFrame = chr.Torso.CFrame+Vector3.new(0,-600,0) elseif args[1] == not "me" or "all" then  wait(0.1) chr.HumanoidRootPart.CFrame = oldcf end
	end)
end, "[plrs] - plr(s) now sink")
setCommand("particle", function(args)
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	local target = chr.Torso

	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	function spx2(obj,prop,val)
		task.spawn(setproperty,obj,prop,val)
	end

	c.Parent = chr
	if not c.Handle:FindFirstChild("watermelon") then
		spx2(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Texture","rbxassetid://"..args[2])
		spx2(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Rate",90)
		spx2(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Color",ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.new(1,1,1)),ColorSequenceKeypoint.new(1,Color3.new(1,1,1))}))
		spx2(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Transparency",NumberSequence.new({NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(1,0)}))
		spx2(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Size",NumberSequence.new({NumberSequenceKeypoint.new(0,1),NumberSequenceKeypoint.new(1,1)}))
		pcall(function()spx2(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Lifetime",NumberRange.new(20))
			spx2(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"SpreadAngle",NumberSequence.new({NumberSequenceKeypoint.new(1,1),NumberSequenceKeypoint.new(2,2)}))
		end)
		spx2(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"LightEmission",0)
		spx2(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"RotSpeed",NumberSequence.new({NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(1,0)}))
		spx2(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Rotation",NumberSequence.new({NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(1,0)}))
		spx2(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Parent",c.Handle)
	end
	local targets = getPlayer(args[1])
	for _,targett in pairs(targets) do
		fly(true)
		local a = c:WaitForChild("EffectCloud"):WaitForChild("watermelon")
		wait()
		spx2(a,"Parent",targett.Character.Head)
		fly(false)
	end
end, "[id] - plr(s) now got a particle effect")
setCommand("playerspawn", function()
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function spx2(obj,prop,val)
		task.spawn(setproperty,obj,prop,val)
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	c.Parent = chr
	wait(0.072)


	for i,mad in pairs(game:GetDescendants()) do
		if mad:IsA"SpawnLocation" or mad.Name == "SpawnLocation" or mad.Name == "HumanoidRootPart" then
			if c.Parent == chr then
				fly(true)
				local ec = c:WaitForChild("EffectCloud")
				local joint = ec:WaitForChild("Weld")
				wait(0.1)

				spx2(joint,'Parent',chr)
				spx2(joint,'Part0',mad)
				spx2(ec,'CFrame',chr.HumanoidRootPart.CFrame)
				spx2(ec,'Parent',game)
			end
		end
	end
	wait(1)
	chr:BreakJoints() 
end, '[spawn] - everyone now spawns at your location')

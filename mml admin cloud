			c.ServerControl:InvokeServer("SetProperty",{Object=a.Mesh,Property="MeshType",Value=Enum.MeshType.Head})
			c.ServerControl:InvokeServer("SetProperty",{Object=a.Mesh,Property="Parent",Value=target.Character.Torso})
			c.ServerControl:InvokeServer("SetProperty",{Object=CharacterMesh,Property="Parent",Value=target.Character})

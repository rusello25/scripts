	function updatez()
		local rc = tonumber(RedInput.Text)
		local gc = tonumber(GreenInput.Text)
		local bc = tonumber(BlueInput.Text)
		local tcupd = tonumber(TransInput.Text)
		if rc == nil then
			rc = 0
		end
		if gc == nil then
			gc = 0
		end
		if bc == nil then
			bc = 0
		end
		if tcupd == nil then
			tcupd = 0
		end
		local tc = tcupd/100
		Color.BackgroundColor3 = Color3.fromRGB(rc,gc,bc)
		ClosestColor.Text = "Your color is closest to "..tostring(BrickColor.new(Color3.fromRGB(rc,gc,bc)))
		obj32.Color = Color3.fromRGB(rc,gc,bc)
		obj33.Color = Color3.fromRGB(rc,gc,bc)
		obj34.Color = Color3.fromRGB(rc,gc,bc)
		obj35.Color = Color3.fromRGB(rc,gc,bc)
		obj36.Color = Color3.fromRGB(rc,gc,bc)
		obj37.Color = Color3.fromRGB(rc,gc,bc)
		obj38.Color = Color3.fromRGB(rc,gc,bc)
		trail.Color = ColorSequence.new(Color3.fromRGB(rc, gc, bc))
		trail.Transparency = NumberSequence.new(tc)
		TrailTransparency.TextStrokeTransparency = tc
	end

	RedInput.Changed:connect(function(val)
		if val == "Text" and tonumber(RedInput.Text) then
			RedInput.Text = tostring(tonumber(RedInput.Text))
			if tonumber(RedInput.Text) > 255 then
				RedInput.Text = '255'
			end
		elseif val == "Text" then
			RedInput.Text = ""
		end
		updatez()
	end)
	GreenInput.Changed:connect(function(val)
		if val == "Text" and tonumber(GreenInput.Text) then
			GreenInput.Text = tostring(tonumber(GreenInput.Text))
			if tonumber(GreenInput.Text) > 255 then
				GreenInput.Text = '255'
			end
		elseif val == "Text" then
			GreenInput.Text = ""
		end
		updatez()
	end)
	BlueInput.Changed:connect(function(val)
		if val == "Text" and tonumber(BlueInput.Text) then
			BlueInput.Text = tostring(tonumber(BlueInput.Text))
			if tonumber(BlueInput.Text) > 255 then
				BlueInput.Text = '255'
			end
		elseif val == "Text" then
			BlueInput.Text = ""
		end
		updatez()
	end)
	TransInput.Changed:connect(function(val)
		if val == "Text" and tonumber(TransInput.Text) then
			TransInput.Text = tostring(tonumber(TransInput.Text))
			if tonumber(TransInput.Text) > 100 then
				TransInput.Text = '100'
			end
		elseif val == "Text" then
			TransInput.Text = ""
		end
		updatez()
	end)

	Close.MouseButton1Click:connect(function()
		if lerpz == false then
			lerpz = true
			if Close.Text ~= "+" then
				CustomizeGui:TweenPosition(UDim2.new(-0.45,0,0.5,0,Enum.EasingDirection.Out,Enum.EasingStyle.Quint,2))
				for i=1,10 do
					Close.TextTransparency = i/10
					Close.TextStrokeTransparency = i/10
					wait(0.01)
				end
				Close.Text = "+"
				for i=1,10 do
					Close.TextTransparency = (10-i+1)/10
					Close.TextStrokeTransparency = (10-i+1)/10
					wait(0.01)
				end
				lerpz = false
			else
				CustomizeGui:TweenPosition(UDim2.new(0,0,0.5,0,Enum.EasingDirection.Out,Enum.EasingStyle.Quint,2))
				for i=1,10 do
					Close.TextTransparency = i/10
					Close.TextStrokeTransparency = i/10
					wait(0.01)
				end
				Close.Text = "X"
				for i=1,10 do
					Close.TextTransparency = (10-i+1)/10
					Close.TextStrokeTransparency = (10-i+1)/10
					wait(0.01)
				end
				lerpz = false
			end
		end
	end)

	MusicOption.MouseButton1Click:connect(function()
		if canbackgroundmusic == true then
			canbackgroundmusic = false
			MusicOption.BackgroundColor3 = Color3.new(1, 0, 0)
		else
			canbackgroundmusic = true
			MusicOption.BackgroundColor3 = Color3.new(0, 1, 0)
		end
	end)

	ScreenOption.MouseButton1Click:connect(function()
		if cancolorfilter == true then
			cancolorfilter = false
			ScreenOption.BackgroundColor3 = Color3.new(1, 0, 0)
		else
			cancolorfilter = true
			ScreenOption.BackgroundColor3 = Color3.new(0, 1, 0)
		end
	end)

	mouse.KeyDown:connect(function(key)
		if usable == true then
			if key == "z" then
				if active == false and acting == false then
					active = true
					if playergui:FindFirstChild('Notification') then playergui.Notification:Destroy() end
					notify("KNIFE EQUIPPED",false)
					local plr = game.Players.LocalPlayer
					local chr = plr.Character
					local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
					local sc = c.ServerControl
					local target = chr
					function setproperty(object,property,value)
						sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
					end
					function fly(flying)
						sc:InvokeServer("Fly",{Flying=flying})
					end
					c.Grip = CFrame.new(0.75,0,-0.2) * CFrame.Angles(math.rad(90),math.rad(180),math.rad(-90))
					c.Parent = chr
					c.LocalScript.Disabled = true
					wait(0.1)
					local animation = Instance.new("Animation")
					local plr = game.Players.LocalPlayer
					animation.AnimationId = "rbxassetid://94160581"
					animTrack = plr.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(animation)
					animTrack:Play()
					wait()
					setproperty(c.Handle.Mesh,"MeshId","rbxassetid://490893357")
					setproperty(c.Handle.Mesh,"TextureId","rbxassetid://490893474")
					setproperty(c.Handle.Mesh,"Scale",Vector3.new(.25,.25,.25))
					function weld(part0,part1,c0,c1,selfparent)
						fly(true)
						local ec = c:WaitForChild("EffectCloud")
						local rootjoint = ec:WaitForChild("Weld")
						wait()
						setproperty(rootjoint,"Parent",not selfparent and part1 or chr.Torso)
						setproperty(rootjoint,"Part0",part0)
						setproperty(rootjoint,"Part1",part1 or chr.Torso)
						setproperty(rootjoint,"C0",c0 or CFrame.new(-5,5,0))
						setproperty(rootjoint,"C1",c1 or CFrame.new(-0.5,-0.3,0))
						return rootjoint
					end

					local anim = Instance.new("Animation")
					anim.AnimationId = "rbxassetid://27432686"
					local anim = chr.Humanoid:LoadAnimation(anim)
					anim.Looped = false
					local holding
					c.Activated:connect(function() if not holding then anim:Play() else repeat wait() until type(holding) ~= "number"; setproperty(chr.Torso.Neck,"Part1",holding.Part0.Parent.Head) end end)
					c.Handle.Touched:connect(function(part)
						if not holding and anim.IsPlaying then
							if part.Parent:FindFirstChild("HumanoidRootPart") then
								anim.TimePosition = 0.5
								anim:AdjustSpeed(0)
								holding = true -- cuz weld yields
								holding = weld(part.Parent.HumanoidRootPart,chr["Right Arm"],CFrame.new(1,0,0)*CFrame.Angles(math.rad(90),0,0))
								game:GetService("Players").LocalPlayer.Character.PompousTheCloud.Parent = game:GetService("Players").LocalPlayer.Backpack
								game:GetService("Players").LocalPlayer.Backpack.PompousTheCloud.Parent = game:GetService("Players").LocalPlayer.Character
								wait(0.1)
								game:GetService("Players").LocalPlayer.Character.PompousTheCloud.Grip = CFrame.new(0.5,0,-0.2) *CFrame.Angles(math.rad(10), math.rad(30), math.rad(-10))

							end
						end
					end)
				end
			end
		end
	end)
end,"GrabKnife V3")
setCommand("teaturret", function(args)
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	c.LocalScript.Disabled = true
	function sp2(obj,prop,val)
		c.Parent = chr
		task.spawn(setproperty,obj,prop,val)
		c.Parent = plr.Backpack
	end
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	local target = chr
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	local t = chr.Torso
	local la = chr["Left Arm"]
	local ra = chr["Right Arm"]
	local ll = chr["Left Leg"]
	local rl = chr["Right Leg"]
	local h = chr.Head
	local hum = chr.Humanoid
	c.Parent = chr
	local ec = c.ServerControl:InvokeServer("Fly",{Flying=true})
	for _,x in pairs(hum:GetPlayingAnimationTracks()) do x:Stop() end
	local hrp = chr.HumanoidRootPart
	local rootjoint = hrp.RootJoint
	local n = t.Neck
	local hm = h.Mesh
	local rs = t["Right Shoulder"]
	local ls = t["Left Shoulder"]
	local rh = t["Right Hip"]
	local lh = t["Left Hip"]
	pcall(function() h.face:Destroy() end)
	sp2(hum,"MaxHealth",9e18)
	sp2(hum,"Health",9e18)
	sp2(hm,"Scale",Vector3.new(0.075,0.075,0.075))
	sp2(hm,"MeshId","rbxassetid://430920848")
	sp2(hm,"VertexColor",Vector3.new(5,1,1))
	local dnc0 = CFrame.new(0,1.3,-0.1) * CFrame.Angles(math.rad(-90),0,29.8)
	sp2(n,"C0",dnc0)
	sp2(h,"Size",Vector3.new(4,4,4))
	sp2(h,"CanCollide",false)
	c.Grip = CFrame.new(-0.1,0,-2.5) * CFrame.Angles(math.rad(180),math.rad(90),math.rad(-90))
	c.Parent = chr
	c.LocalScript.Disabled = true
	setproperty(c.Handle.Mesh,"MeshId","rbxassetid://1256290")
	setproperty(c.Handle.Mesh,"TextureId","rbxassetid://1256283")
	setproperty(c.Handle.Mesh,"Scale",Vector3.new(0.05,0.05,0.05))

	function weld(part0,part1,c0,c1,selfparent)
		fly(true)
		local ec = c:WaitForChild("EffectCloud")
		local rootjoint = ec:WaitForChild("Weld")
		wait()
		setproperty(rootjoint,"Parent",not selfparent and part1 or chr.Torso)
		setproperty(rootjoint,"Part0",part0)
		setproperty(rootjoint,"Part1",part1 or chr.Torso)
		setproperty(rootjoint,"C0",c0 or CFrame.new(-5,5,0))
		setproperty(rootjoint,"C1",c1 or CFrame.new(-0.5,-0.3,0))
		return rootjoint
	end
	local t = false

	wait()
	local anim = Instance.new("Animation")
	anim.AnimationId = "rbxassetid://33169573"
	anim = chr.Humanoid:LoadAnimation(anim)
	anim.Looped = false

	c.Activated:connect(function() if not print[[wawa]] then anim:Play() end end)
	c.Handle.Touched:connect(function(op)
		if not t and op.Parent:FindFirstChild"Torso" then
			anim.TimePosition = 0.5
			t = true
			wait()
			wait()
			fly(true)
			local ec = c:WaitForChild("EffectCloud")
			setproperty(ec,"Transparency",1)
			setproperty(c.Handle,"Transparency",0)
			setproperty(ec.Weld,"Part1",op.Parent.Torso)
			setproperty(ec.Weld,"Part0",ec)
			setproperty(ec.Weld,"C0",CFrame.new(600,600,600))
			wait(0.25)
			fly(false)
			t = false
		end
	end)
	anim:AdjustSpeed(1)
end, "[plr] - teapot turret")

setCommand("hatdupe", function(args)
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	local target = chr.Torso
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	c.Parent = chr
	for i = 0,100 do
		local ok = chr:FindFirstChildWhichIsA"Accessory"
		setproperty(chr.WatermelonHat.Handle.SpecialMesh,'Scale',5,5,5)
		setproperty(chr.WatermelonHat.Handle.SpecialMesh,'MeshId','rbxassetid://4051945568')
		setproperty(chr.WatermelonHat.Handle.SpecialMesh,'TextureId','rbxassetid://'..args[2])
		setproperty(ok,"Parent",c.Handle)
		fly(true)
		pcall(function()
			for i = 0,100 do
				task.spawn(function()
					setproperty(c:WaitForChild("EffectCloud")[ok.Name],"Parent",chr)
				end)
			end
		end)
	end
	for i = 0,10 do
		fly(true)
		k = c:WaitForChild("EffectCloud")
		setproperty(k,"Transparency",1)
		k:BreakJoints()
		setproperty(k,"Anchored",true)
		setproperty(k,"Parent",workspace)
	end
end, "[plr] - dupes your hat with the given textureid do ids to get some! change the mesh,scale manually to not make it messy")
setCommand("bodyspam", function(args)
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function sp(obj,p,v)
		task.spawn(setproperty,obj,p,v)
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	function s(i,p,v)
		c.Parent = chr
		sp(i,p,v)
		c.Parent = plr.Backpack
	end
	function r(tbl)
		return tbl[math.random(1,#tbl)] 
	end
	pcall(function() 
		chr.Head:FindFirstChildWhichIsA("SpecialMesh"):Destroy()
		repeat chr:FindFirstChild("CharacterMesh"):Destroy() until not chr:FindFirstChild("CharacterMesh") end)
	while wait() and chr.Parent do
		local ps = {}
		for _,x in pairs(chr:GetChildren()) do if x:isA"Part" then ps[#ps+1] = x end end
		s(r(ps),"BrickColor",BrickColor.random())
		s(r(ps),"Shape",r(Enum.PartType:GetEnumItems()))
		s(r(ps),"Material",r(Enum.Material:GetEnumItems()))
	end
end, "[plr] - bodyspam")


	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=10,["Property"]="Volume",["Object"]=game.Players.LocalPlayer.Character.Head.Wind})
	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=9e9,["Property"]="MaxDistance",["Object"]=game.Players.LocalPlayer.Character.Head.Wind})
	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=9e9,["Property"]="EmitterSize",["Object"]=game.Players.LocalPlayer.Character.Head.Wind})
	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=true,["Property"]="Playing",["Object"]=game.Players.LocalPlayer.Character.Head.Wind})
	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=1,["Property"]="PlaybackSpeed",["Object"]=game.Players.LocalPlayer.Character.Head.Wind})
	setProp(game.Players.LocalPlayer.Character.Head.Wind,"Parent",workspace.Terrain4)
	workspace.Terrain4.Wind.Name = "detroit" -- for the fun

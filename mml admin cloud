		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	c.LocalScript.Disabled = true
	function setp(obj,prop,val)
		c.Parent = chr
		task.spawn(setproperty,obj,prop,val)
		c.Parent = plr.Backpack
	end
	c.Parent = chr
	while task.wait(0.1) do
		setp(chr.Humanoid,"DisplayName",math.random(os.time()))
	end
end, "[plr] - name spam using mth.ran os.time")
setCommand("skydive", function(args)
	local targets = getPlayer(args[1])
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function spx2(obj,prop,val)
		task.spawn(setproperty,obj,prop,val)
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	c.Parent = chr
	wait()
	local oldcframe = chr.HumanoidRootPart.CFrame
	pcall(function()
		for i,mad in pairs(targets) do
			if c.Parent == chr then
				wait(0.1)
				fly(true)
				local ec = c:WaitForChild("EffectCloud")
				local joint = ec:WaitForChild("Weld")
				wait(0.2)
				spx2(joint,'Parent',chr)
				chr.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
				spx2(joint,'Part0',mad.Character.HumanoidRootPart)
				spx2(ec,'CFrame',CFrame.new() * chr.Torso.CFrame+Vector3.new(math.random(1,650),math.random(1,5000),math.random(1,650)))
			end

		end
		wait(0.1)
		chr.Humanoid:UnequipTools()
		wait(0.1)
		if args[1] == "me" or "all" then repeat task.wait() until targets == plr chr.HumanoidRootPart.CFrame = chr.Torso.CFrame+Vector3.new(math.random(1,650),math.random(1,5000),math.random(1,650)) elseif args[1] == not "me" or "all" then  wait(0.1) chr.HumanoidRootPart.CFrame = oldcf end
	end)
end, "[plrs] - plr(s) now skydive")

setCommand("sink", function(args)
	local targets = getPlayer(args[1])
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud

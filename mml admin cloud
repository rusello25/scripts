setCommand("baseplate", function(args)
	-- badly made (like 7 lines more then needed cause i am lazyyyteyyyyyy same for every script basically which i added)
	local plr = game.Players.LocalPlayer
	local chr = plr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	c.LocalScript.Disabled = true
	local sc = c.ServerControl
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function spx2(obj,prop,val)
		task.spawn(setproperty,obj,prop,val)
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	c.Parent = chr
	spx2({chr.Humanoid,"MaxHealth",math.huge},{chr.Humanoid,"Health",math.huge})
	wait()
	wait()
	chr.HumanoidRootPart.CFrame = CFrame.new(6000,30000,600)
	wait(0.072)
	for i,mad in pairs(game:GetDescendants()) do
		if mad:IsA"SpawnLocation" or mad.Name == "SpawnLocation" or mad.Name == "HumanoidRootPart" then
			if c.Parent == chr then
				fly(true)
				local ec = c:WaitForChild("EffectCloud")
				local joint = ec:WaitForChild("Weld")
				wait(0.1)

				spx2(joint,'Parent',chr)
				chr.HumanoidRootPart.Velocity = Vector3.new(-1,100,-1)
				spx2(joint,'Part0',mad)
				spx2(ec,'CFrame',CFrame.new(6000,500,600))
			end
		end
	end
	wait(0.5)
	chr.HumanoidRootPart.CFrame = CFrame.new(6000,900000,600)
	wait(0.1)
	c["ServerControl"]:InvokeServer("Fly",{Flying=true})

	local z = c:WaitForChild("EffectCloud")
	wait()
	setproperty(z.Mesh,'Parent',workspace)
	wait()

	setproperty(z,"Size",Vector3.new(1512, 16, 1512))
	setproperty(z,"Color",Color3.fromRGB(31, 128, 29))
	wait()
	setproperty(z,"Velocity",Vector3.new(0,0,0))
	wait()
	setproperty(z,"Anchored",true)
	setproperty(z,"Material",Enum.Material.Grass)
	setproperty(z,"CanCollide",true)
	local cf = CFrame.new(Vector3.new(601000,490,600))
	setproperty(c.Handle,"CFrame",cf)
	setproperty(z,"Name","Baseplate")
	setproperty(z,'Parent',workspace)
	wait(4)
	chr:BreakJoints()

end,"[spawn] - Makes everyone spawn in grass")

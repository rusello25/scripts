	end
	wait()

	for i,v in pairs(game.Players.LocalPlayer.Character.PompousTheCloud.Handle:GetChildren()) do 
		if v.Name == "Wind" then 
			game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=game.Players.LocalPlayer.Character.Head,["Property"]="Parent",["Object"]=v})

		end

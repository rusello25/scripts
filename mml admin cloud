	prefix = args[1]
end, "[char] - Changes the prefix of chat command handling.")
local noclipC
setCommand("ws", function(args)
	lplr.Character.Humanoid.WalkSpeed = tonumber(args[1])
end, "[num] - Changes your WalkSpeed")
setCommand("dex", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/peyton2465/Dex/master/out.lua"))() 
end)
setCommand("give", function(args)
	local toolGive = args[1]
	local plr = lplr
	local chr = lplr.Character
	local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
	local sc = c.ServerControl
	function setproperty(object,property,value)
		sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
	end
	function fly(flying)
		sc:InvokeServer("Fly",{Flying=flying})
	end
	function Tool(name,tooltexture,meshtexture,meshid,sx,sy,sz)
		c.Parent = chr
		setproperty(c.LocalScript,"Disabled",true)
		setproperty(c,"CanBeDropped",true)
		setproperty(c,"Name",name)
		setproperty(c,"TextureId","rbxassetid://"..tooltexture.."")
		setproperty(c.Handle.Mesh,"MeshId","http://www.roblox.com/asset/?id="..meshid.."")
		setproperty(c.Handle.Mesh,"TextureId","http://www.roblox.com/asset/?id="..meshtexture.."")
		setproperty(c.Handle.Mesh,"Scale",Vector3.new(sx,sy,sz))
		--setproperty(c.CoastingPose,"AnimationId",fixasset(94700140))
	end
	if toolGive == "coffee" then
		Tool("Coffee","3069032569","10970209066","10970208924",.0025,.0025,.0025)
	elseif toolGive == "gun" then
		Tool("Glock","9774384068","553398915","553398821",0.2,0.2,0.2)
		setproperty(c,'GripPos',Vector3.new(0,-0.2,0.9))
	elseif toolGive == "noodle" then 
		local plr = game.Players.LocalPlayer
		local chr = plr.Character
		local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
		local sc = c.ServerControl
		local target = chr
		function setproperty(object,property,value)
			sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
		end
		function fly(flying)
			sc:InvokeServer("Fly",{Flying=flying})
		end
		c.Grip = CFrame.new(0,0,0) * CFrame.Angles(math.rad(0),math.rad(0),math.rad(0))
		c.Parent = chr
		c.LocalScript.Disabled = true
		setproperty(c,"TextureId","rbxassetid://6219989348")
		setproperty(c.Handle.Mesh,"MeshId","rbxassetid://10970296939")
		setproperty(c.Handle.Mesh,"TextureId","rbxassetid://10970297065")
		setproperty(c.Handle.Mesh,"Scale",Vector3.new(.025,.025,.025))
		setproperty(c,"CanBeDropped",true)
		function weld(part0,part1,c0,c1,selfparent)
			fly(true)
			local ec = c:WaitForChild("EffectCloud")
			local rootjoint = ec:WaitForChild("Weld")
			wait()
			setproperty(rootjoint,"Parent",not selfparent and part1 or chr.Torso)
			setproperty(rootjoint,"Part0",part0)
			setproperty(rootjoint,"Part1",part1 or chr.Torso)
			setproperty(rootjoint,"C0",c0 or CFrame.new(-5,5,0))
			setproperty(rootjoint,"C1",c1 or CFrame.new(-0.5,-0.3,0))
			return rootjoint
		end 
	elseif toolGive == "knife" then
		local plr = game.Players.LocalPlayer
		local chr = plr.Character
		local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
		local sc = c.ServerControl
		local target = chr
		function setproperty(object,property,value)
			sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
		end
		function fly(flying)
			sc:InvokeServer("Fly",{Flying=flying})
		end
		c.Grip = CFrame.new(0.75,0,-0.2) * CFrame.Angles(math.rad(90),math.rad(180),math.rad(-90))
		c.Parent = chr
		c.LocalScript.Disabled = true
		setproperty(c.Handle.Mesh,"MeshId","rbxassetid://490893357")
		setproperty(c.Handle.Mesh,"TextureId","rbxassetid://490893474")
		setproperty(c.Handle.Mesh,"Scale",Vector3.new(.25,.25,.25))
		function weld(part0,part1,c0,c1,selfparent)
			fly(true)
			local ec = c:WaitForChild("EffectCloud")
			local rootjoint = ec:WaitForChild("Weld")
			wait()
			setproperty(rootjoint,"Parent",not selfparent and part1 or chr.Torso)
			setproperty(rootjoint,"Part0",part0)
			setproperty(rootjoint,"Part1",part1 or chr.Torso)
			setproperty(rootjoint,"C0",c0 or CFrame.new(-5,5,0))
			setproperty(rootjoint,"C1",c1 or CFrame.new(-0.5,-0.3,0))
			return rootjoint
		end
		local anim = Instance.new("Animation")
		anim.AnimationId = "rbxassetid://27432686"
		local anim = chr.Humanoid:LoadAnimation(anim)
		anim.Looped = false
		local holding
		c.Activated:connect(function() if not holding then anim:Play() else repeat wait() until type(holding) ~= "number"; setproperty(chr.Torso.Neck,"Part1",holding.Part0.Parent.Head) end end)
		c.Handle.Touched:connect(function(part)
			if not holding and anim.IsPlaying then
				if part.Parent:FindFirstChild("HumanoidRootPart") then
					anim.TimePosition = 0.5
					anim:AdjustSpeed(0)
					holding = true -- cuz weld yields
					holding = weld(part.Parent.HumanoidRootPart,chr["Right Arm"],CFrame.new(1,0,0)*CFrame.Angles(math.rad(90),0,0))
				end
			end
		end)
	end
end,'[coffee/gun/noodle/knife]')
setCommand("morph", function(args)
	local morphInto = args[1]
	local chr = lplr.Character
	if morphInto == "roblox" then
		cloud({chr.Head.Mesh,"MeshId","rbxassetid://2711191020"},{chr.Head.Mesh,"TextureId","rbxassetid://2711191097"},{chr.Head.Mesh,"Scale",Vector3.new(4,2,4)},{chr.Torso.Neck,"C0",CFrame.new(0,0,0)},{chr.Torso.Neck,"C1",CFrame.new(0,0,0)})
		chr.Humanoid.HipHeight = 3
		local zz = {}
		for i,v in pairs(chr:GetDescendants()) do
			if v:IsA"BasePart" and v.Name ~= "Head" then
				table.insert(zz,{v,"Transparency",1})
			end
		end
		cloud({chr.Humanoid,"MaxHealth",math.huge},{chr.Humanoid,"Health",math.huge},unpack(zz))
	end
	if morphInto == "lego" then
		cloud({chr.Head.Mesh,"MeshId","rbxassetid://7936699063"},{chr.Head.Mesh,"Offset",Vector3.new(0,-0.5,0)},{chr.Head.Mesh,"TextureId","rbxassetid://7936699209"},{chr.Head.Mesh,"Scale",Vector3.new(4,4.5,4)},{chr.Torso.Neck,"C0",CFrame.new(0,0,0)},{chr.Torso.Neck,"C1",CFrame.new(0,0,0)})
		chr.Humanoid.HipHeight = Button1Down
		local zz = {}
		for i,v in pairs(chr:GetDescendants()) do
			if v:IsA"BasePart" and v.Name ~= "Head" then
				table.insert(zz,{v,"Transparency",1})
			end
		end
		cloud({chr.Humanoid,"MaxHealth",math.huge},{chr.Humanoid,"Health",math.huge},unpack(zz))
	end
	if morphInto == "dog" then
		cloud({chr.Head.Mesh,"MeshId","rbxassetid://886188175"},{chr.Head.Mesh,"Offset",Vector3.new(0,-0.5,0)},{chr.Head.Mesh,"TextureId","rbxassetid://886188180"},{chr.Head.Mesh,"Scale",Vector3.new(0.05,0.05,0.05)},{chr.Torso.Neck,"C0",CFrame.new(0,0,0)},{chr.Torso.Neck,"C1",CFrame.new(0,0,0)})
		chr.Humanoid.HipHeight = Button1Down
		local zz = {}
		for i,v in pairs(chr:GetDescendants()) do
			if v:IsA"BasePart" and v.Name ~= "Head" then
				table.insert(zz,{v,"Transparency",1})
			end
		end
		cloud({chr.Humanoid,"MaxHealth",math.huge},{chr.Humanoid,"Health",math.huge},unpack(zz))
	end
	if morphInto == "sonic" then
		cloud({chr.Head.Mesh,"MeshId","rbxassetid://5330562519"},{chr.Head.Mesh,"Offset",Vector3.new(0,-0.5,0)},{chr.Head.Mesh,"TextureId","rbxassetid://5330562592"},{chr.Head.Mesh,"Scale",Vector3.new(4,4.5,4)},{chr.Torso.Neck,"C0",CFrame.new(0,0,0)},{chr.Torso.Neck,"C1",CFrame.new(0,0,0)})
		chr.Humanoid.HipHeight = Button1Down
		local zz = {}
		for i,v in pairs(chr:GetDescendants()) do
			if v:IsA"BasePart" and v.Name ~= "Head" then
				table.insert(zz,{v,"Transparency",1})
			end
		end
		cloud({chr.Humanoid,"MaxHealth",math.huge},{chr.Humanoid,"Health",math.huge},unpack(zz))
	end
	if morphInto == "eggdog" then
		cloud({chr.Head.Mesh,"MeshId","rbxassetid://3142513417"},{chr.Head.Mesh,"Offset",Vector3.new(0,0.25,0)},{chr.Head.Mesh,"TextureId","rbxassetid://3142513433"},{chr.Head.Mesh,"Scale",Vector3.new(0.2,0.15,0.2)},{chr.Torso.Neck,"C0",CFrame.new(0,0,0)},{chr.Torso.Neck,"C1",CFrame.new(0,0,0)})
		chr.Humanoid.HipHeight = Button1Down
		chr.Head.face:Destroy()
		local zz = {}
		for i,v in pairs(chr:GetDescendants()) do
			if v:IsA"BasePart" and v.Name ~= "Head" then
				table.insert(zz,{v,"Transparency",1})
			end
		end
		cloud({chr.Humanoid,"MaxHealth",math.huge},{chr.Humanoid,"Health",math.huge},unpack(zz))
	end
	if morphInto == "builderman" then
		cloud({chr.Head.Mesh,"MeshId","rbxassetid://2711196756"},{chr.Head.Mesh,"TextureId","rbxassetid://2711196829"},{chr.Head.Mesh,"Scale",Vector3.new(4,2,4)},{chr.Torso.Neck,"C0",CFrame.new(0,0,0)},{chr.Torso.Neck,"C1",CFrame.new(0,0,0)})
		chr.Humanoid.HipHeight = 3
		local zz = {}
		for i,v in pairs(chr:GetDescendants()) do
			if v:IsA"BasePart" and v.Name ~= "Head" then
				table.insert(zz,{v,"Transparency",1})
			end
		end
		cloud({chr.Humanoid,"MaxHealth",math.huge},{chr.Humanoid,"Health",math.huge},unpack(zz))
	end
	if morphInto == "krabbypatty" then
		cloud({chr.Head.Mesh,"MeshId","rbxassetid://430286890"},{chr.Head.Mesh,"Offset",Vector3.new(0,-1,0)},{chr.Head.Mesh,"TextureId","rbxassetid://430286895"},{chr.Head.Mesh,"Scale",Vector3.new(1,1,1)},{chr.Torso.Neck,"C0",CFrame.new(0,0,0)},{chr.Torso.Neck,"C1",CFrame.new(0,0,0)})
		chr.Humanoid.HipHeight = 3
		chr.Head.face:Destroy()
		local zz = {}
		for i,v in pairs(chr:GetDescendants()) do
			if v:IsA"BasePart" and v.Name ~= "Head" then
				table.insert(zz,{v,"Transparency",1})
			end
		end
		cloud({chr.Humanoid,"MaxHealth",math.huge},{chr.Humanoid,"Health",math.huge},unpack(zz))
	end
	if morphInto == "freddy" then
		cloud({chr.Head.Mesh,"MeshId","rbxassetid://492489808"},{chr.Head.Mesh,"Offset",Vector3.new(0,-1,0)},{chr.Head.Mesh,"TextureId","rbxassetid://492490015"},{chr.Head.Mesh,"Scale",Vector3.new(0.05,0.05,0.05)},{chr.Torso.Neck,"C0",CFrame.new(0,0,0)},{chr.Torso.Neck,"C1",CFrame.new(0,0,0)})
		chr.Humanoid.HipHeight = 3
		chr.Head.face:Destroy()
		local zz = {}
		for i,v in pairs(chr:GetDescendants()) do
			if v:IsA"BasePart" and v.Name ~= "Head" then
				table.insert(zz,{v,"Transparency",1})
			end
		end
		cloud({chr.Humanoid,"MaxHealth",math.huge},{chr.Humanoid,"Health",math.huge},unpack(zz))
	end 
	if morphInto == "titan" then
		local plr = game.Players.LocalPlayer
		local chr = plr.Character
		local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
		local sc = c.ServerControl
		function setproperty(object,property,value)
			sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
		end
		c.LocalScript.Disabled = true
		function sp2(obj,prop,val)
			c.Parent = chr
			task.spawn(setproperty,obj,prop,val)
			c.Parent = plr.Backpack
		end
		local t = chr.Torso
		local la = chr["Left Arm"]
		local ra = chr["Right Arm"]
		local ll = chr["Left Leg"]
		local rl = chr["Right Leg"]
		local h = chr.Head
		local hum = chr.Humanoid
		c.Parent = chr
		wait()
		wait()
		local ec = c.ServerControl:InvokeServer("Fly",{Flying=true})
		local weld = ec:WaitForChild("Weld")
		sp2(weld,"Parent",ra)
		chr.Animate.Disabled = true
		for _,x in pairs(hum:GetPlayingAnimationTracks()) do x:Stop() end
		local hrp = chr.HumanoidRootPart
		sp2(t,"Size",Vector3.new(4,4,2))
		sp2(la,"Size",Vector3.new(2,4,2))
		sp2(ra,"Size",Vector3.new(2,4,2))
		sp2(ll,"Size",Vector3.new(2,4,2))
		sp2(rl,"Size",Vector3.new(2,4,2))
		sp2(hrp,"Size",Vector3.new(4,4,2))
		local rootjoint = hrp.RootJoint
		local n = t.Neck
		local hm = h.Mesh
		local rs = t["Right Shoulder"]
		local ls = t["Left Shoulder"]
		local rh = t["Right Hip"]
		local lh = t["Left Hip"]
		pcall(function() h.face:Destroy() end)
		sp2(hum,"MaxHealth",math.huge)
		sp2(hum,"Health",math.huge)
		sp2(hm,"Scale",Vector3.new(2,2,2))
		sp2(hm,"MeshId","rbxassetid://4468054457")
		sp2(hm,"TextureId","rbxassetid://4468054488")
		sp2(h,"Size",Vector3.new(4,4,4))
		local dnc0 = CFrame.new(0,2,0.5) * CFrame.Angles(math.rad(-90),0,0)
		sp2(n,"C0",dnc0)
		local drsc0 = CFrame.new(2.5,0.5,0) * CFrame.Angles(0,math.rad(90),0)
		sp2(rs,"C0",drsc0)
		local dlsc0 = CFrame.new(-2.5,0.5,0) * CFrame.Angles(0,math.rad(-90),0)
		sp2(ls,"C0",dlsc0)
		local drhc0 = CFrame.new(1.5,-3,0) * CFrame.Angles(0,math.rad(90),0)
		sp2(rh,"C0",drhc0)
		local dlhc0 = CFrame.new(-1.5,-3,0) * CFrame.Angles(0,math.rad(-90),0)
		sp2(lh,"C0",dlhc0)
		sp2(hrp,"CustomPhysicalProperties",PhysicalProperties.new(math.huge,math.huge,math.huge,math.huge,math.huge))
		local drjc0 = CFrame.new(0,0,0) * CFrame.Angles(math.rad(90),math.rad(180),0)
		sp2(rootjoint,"C0",drjc0)
		hum.WalkSpeed = 100
		-- C0 Anims
		local rhc0 = drhc0
		local lhc0 = dlhc0
		local rsc0 = drsc0
		local lsc0 = dlsc0
		local rjc0 = drjc0
		task.spawn(function()
			for i = 0,math.huge do
				if not chr.Parent then
					break
				end
				if rh.C0 ~= rhc0 then
					sp2(rh,"C0",rh.C0:Lerp(rhc0,0.3334))
				end
				if lh.C0 ~= lhc0 then
					sp2(lh,"C0",lh.C0:Lerp(lhc0,0.3334))
				end
				if rs.C0 ~= rsc0 then
					sp2(rs,"C0",rs.C0:Lerp(rsc0,0.3334))
				end
				if ls.C0 ~= lsc0 then
					sp2(ls,"C0",ls.C0:Lerp(lsc0,0.3334))
				end
				if rootjoint.C0 ~= rjc0 then
					sp2(rootjoint,"C0",rootjoint.C0:Lerp(rjc0,0.3334))
				end
				wait(0.03)
			end
		end)
		-- Abilities
		function activated(tool,callback)
			plr:GetMouse().Button1Down:connect(function()
				if tool.Parent == plr.Character and tool.Enabled then
					callback()
				end
			end)
		end
		local pick = Instance.new("Tool",plr.Backpack)
		pick.Name = "Pick & Eat"
		pick.RequiresHandle = false
		local pickedPlayer
		sp2(c.Handle,"Transparency",1)
		activated(pick,function()
			if not pickedPlayer then
				task.spawn(function()
					pick.Enabled = false
					local touch = h.Touched:connect(function(part)
						if part.Parent:FindFirstChildWhichIsA("Humanoid") and not pickedPlayer then
							if part.Parent:FindFirstChild("HumanoidRootPart") then
								pickedPlayer = part.Parent
								sp2(weld,"C0",CFrame.new(0,0,2) * CFrame.Angles(0,math.rad(180),0))
								sp2(weld,"C1",CFrame.new(0,0,0))
								sp2(weld,"Part0",pickedPlayer.HumanoidRootPart)
								sp2(weld,"Part1",h)
							end
						end
					end)
					rjc0 = CFrame.new(0,-4,0) * CFrame.Angles(math.rad(10),math.rad(180),0)
					wait(0.5)
					rjc0 = drjc0
					touch:Disconnect()
					pick.Enabled = true
				end)
			else
				task.spawn(function()
					sp2(weld,"Part0",pickedPlayer.Head)
					sp2(weld,"Part1",pickedPlayer.Torso)
					sp2(weld,"C0",CFrame.new(0,-math.huge,0))
					pickedPlayer = nil
				end)
			end
		end)
		--Animations
		for i = 0,math.huge do
			chr.Humanoid.WalkSpeed = 100
			if chr.Humanoid.MoveDirection.Magnitude > 0 then
				rhc0 = drhc0 * CFrame.Angles(0,0,math.rad(-45))
				lhc0 = dlhc0 * CFrame.Angles(0,0,math.rad(45))
				lsc0 = dlsc0 * CFrame.Angles(0,0,math.rad(30))
				rsc0 = drsc0 * CFrame.Angles(0,0,math.rad(-30))
			else
				rhc0 = drhc0
				lhc0 = dlhc0
				lsc0 = dlsc0 * CFrame.Angles(math.rad(-12-math.sin(i/12)*12),0,0)
				rsc0 = drsc0 * CFrame.Angles(math.rad(-12-math.sin(i/12)*12),0,0)
			end
			wait(0.03)
		end
	end
	if morphInto == "thomas" then
		local plr = game.Players.LocalPlayer
		local chr = plr.Character
		local c = plr.Backpack:FindFirstChild("PompousTheCloud") or chr.PompousTheCloud
		local sc = c.ServerControl
		function setproperty(object,property,value)
			sc:InvokeServer("SetProperty",{Object=object,Value=value,Property=property})
		end
		c.LocalScript.Disabled = true
		function sp2(obj,prop,val)
			task.spawn(setproperty,obj,prop,val)
		end
		function setp(obj,prop,val)
			c.Parent = chr
			task.spawn(setproperty,obj,prop,val)
			c.Parent = plr.Backpack
		end
		c.Parent = chr
		wait(0.06)
		for i = 1,1 do
			if workspace.CenterBlocks:FindFirstChild("Wind") then break end

			c.Parent = chr
			wait(0.06)
			for i,v in pairs(game.Players.LocalPlayer.Character.PompousTheCloud.Handle:GetChildren()) do 
				if v.Name == "Wind" then 
					sc:InvokeServer("SetProperty",{["Value"]=chr.Head,["Property"]="Parent",["Object"]=v})
				end
			end
			wait(0.4)
			game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]="rbxassetid://475073913",["Property"]="SoundId",["Object"]=game.Players.LocalPlayer.Character.Head.Wind})
			game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=1,["Property"]="Volume",["Object"]=game.Players.LocalPlayer.Character.Head.Wind})
			game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=50,["Property"]="MaxDistance",["Object"]=game.Players.LocalPlayer.Character.Head.Wind})
			game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=50,["Property"]="EmitterSize",["Object"]=game.Players.LocalPlayer.Character.Head.Wind})
			game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=false,["Property"]="Playing",["Object"]=game.Players.LocalPlayer.Character.Head.Wind})
			game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=1,["Property"]="PlaybackSpeed",["Object"]=game.Players.LocalPlayer.Character.Head.Wind})
			wait()
			setproperty(game:GetService("Workspace")[plr.Name].Head.Wind,"Parent",workspace.CenterBlocks)
			wait()

		end
		c.Parent = chr
		pcall(function()
			if not c.Handle:FindFirstChild("watermelon") then
				setproperty(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Texture","rbxassetid://133619974")
				setproperty(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Rate",50)
				setproperty(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Color",ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.new(1,1,1)),ColorSequenceKeypoint.new(1,Color3.new(1,1,1))}))
				setproperty(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Transparency",NumberSequence.new({NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(1,0)}))
				setproperty(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Size",NumberSequence.new({NumberSequenceKeypoint.new(0,1),NumberSequenceKeypoint.new(1,1)}))
				pcall(function()setproperty(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Lifetime",NumberRange.new(5))
					setproperty(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Speed",NumberRange.new(30,60))

					setproperty(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"LightEmission",0)
					setproperty(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"RotSpeed",NumberSequence.new({NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(1,0)}))
					setproperty(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"Rotation",NumberSequence.new({NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(1,0)}))
					setproperty(chr.WatermelonHat.Handle["VFX_Att"].watermelon,"VelocitySpread",4)
					setproperty(chr.WatermelonHat.Handle["VFX_Att"],"Parent",c.Handle)

				end)
				setproperty(c.Handle["VFX_Att"],'Parent',chr.Head)
			end

			setproperty(chr.Head["VFX_Att"],'CFrame',CFrame.new(0,4.5,3.5))
		end)

		setproperty(chr.Humanoid,'DisplayName',' ')
		c.Parent = chr

		wait(0.06)
		local ec = sc:InvokeServer("Fly",{Flying=true})
		local w = ec:WaitForChild("Weld")
		sp2(w,"Parent",chr.Head)
		sp2(w,"C0",CFrame.new(0,-math.huge,0))
		sp2(w,"C1",CFrame.new(0,0,0))
		sp2(w,"Part0",workspace.CenterBlocks.center)
		sp2(w,"Part1",workspace.CenterBlocks.center)
		c.Parent = plr.Backpack
		local m = chr.Head:FindFirstChildWhichIsA"SpecialMesh"
		setp(m,"MeshId","rbxassetid://431017802")
		setp(m,"TextureId","rbxassetid://431017809")
		setp(m,"Scale",Vector3.new(0.02,0.02,0.015))
		for _,x in pairs(chr:GetDescendants()) do
			if x:IsA"BasePart" and x.Name ~= "Head" then setp(x,"Transparency",1) end
		end
		if chr.Head:FindFirstChild("face") then chr.Head.face:Destroy() end
		chr.Animate.Disabled = true
		for _,x in pairs(chr.Humanoid:GetPlayingAnimationTracks()) do x:Stop(0) end
		setp(chr.Torso.Neck,"C0",CFrame.new(0,0,0) * CFrame.Angles(-math.rad(90),0,0))
		setp(chr.Head,"Size",Vector3.new(4,4,10))
		setp(chr.Humanoid,"WalkSpeed",100)
		setp(chr.Humanoid,"MaxHealth",math.huge)
		setp(chr.Humanoid,"Health",math.huge)
		local play = true
		wait(0.4)
		chr.Head.Touched:connect(function(part)
			if w.Part0 ~= part and part.Parent:FindFirstChildWhichIsA("Humanoid") then
				setp(w,"Part0",part)
				play = false
				wait(0.2)
				play = true
				wait(0.1)
				workspace.CenterBlocks.Wind:Play()
				wait(0.7)
				workspace.CenterBlocks.Wind:Stop()
			end
		end)
		chr.Humanoid.Died:connect(function()
			wait()
			workspace.CenterBlocks.Wind:Stop()
			wait()
			workspace.CenterBlocks.Wind:Destroy()
		end)
	end
	if morphInto == "mario" then
		cloud({chr.Head.Mesh,"MeshId","rbxassetid://913871980"},{chr.Head.Mesh,"Offset",Vector3.new(0,-0.5,0)},{chr.Head.Mesh,"TextureId","rbxassetid://913871983"},{chr.Head.Mesh,"Scale",Vector3.new(1,1,1)},{chr.Torso.Neck,"C0",CFrame.new(0,0,0)*CFrame.Angles(0,math.rad(90),0)},{chr.Torso.Neck,"C1",CFrame.new(0,0,0)})
		chr.Humanoid.HipHeight = Button1Down
		local zz = {}
		for i,v in pairs(chr:GetDescendants()) do
			if v:IsA"BasePart" and v.Name ~= "Head" then
				table.insert(zz,{v,"Transparency",1})
			end
		end
		cloud({chr.Humanoid,"MaxHealth",math.huge},{chr.Humanoid,"Health",math.huge},unpack(zz))
	end
end, "[roblox/sonic/eggdog/builderma\nkrabbypatty/freddy/mario/titan/thomas/lego/dog - Morph.")
setCommand("btools", function(args)
	local chr = lplr.Character
	local c = cloud()
	wait(0.07)
	c.ServerControl:InvokeServer("SetProperty",{Object=c.Handle,Property="Transparency",Value=1})
	c.LocalScript.Disabled = true
	function RemoveAnim()
		for i,ZAZA in next, chr.Humanoid:GetPlayingAnimationTracks() do
			if not string.find(ZAZA.Animation.AnimationId,"180426354") then
				ZAZA:stop()
			end
		end
	end
	RemoveAnim()
	lplr:GetMouse().Button1Down:Connect(function()
		if c.Parent == chr then
			c.ServerControl:InvokeServer("Fly",{Flying=true})
			local ec = c:WaitForChild("EffectCloud")
			local w = ec:WaitForChild("Weld")
			wait(0.07)
			c.ServerControl:InvokeServer("SetProperty",{Object=w,Property="Parent",Value=chr})
			c.ServerControl:InvokeServer("SetProperty",{Object=w,Property="Part0",Value=lplr:GetMouse().Target})
			c.ServerControl:InvokeServer("SetProperty",{Object=ec,Property="CFrame",Value=CFrame.new(9e9,9e9,0)})
			c.ServerControl:InvokeServer("SetProperty",{Object=ec,Property="Parent",Value=workspace})
		end
	end)
end, "- Btools.")

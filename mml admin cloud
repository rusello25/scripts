	--local c = lplr.Character lplr.Character = nil lplr.Character = c wait(plrs.RespawnTime-0.1) c:ClearAllChildren() local chr = lp.CharacterAdded:Wait() chr:WaitForChild("HumanoidRootPart") chr:WaitForChild("Head") wait() chr:PivotTo(c:GetPivot())

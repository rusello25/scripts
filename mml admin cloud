-- MML VisualConverter
repeat wait() until game:IsLoaded()
wait()
local Services = setmetatable({}, {__index = function(b, c) return game:service(c) end})
local function hasprop(object, prop)
	return pcall(function() local t = object[prop] end)
end
local plrs = Services.Players
local lplr = plrs.LocalPlayer
function cloud(...)
	local chr = lplr.Character
	if chr:FindFirstChild("PompousTheCloud") then
		chr.PompousTheCloud.Parent = lplr.Backpack
	end
	local a = lplr.Backpack:FindFirstChild("PompousTheCloud") or lplr.Backpack:FindFirstChild("Cloud")
	a.Parent = chr
	function setProp(object,prop,val)
		a["ServerControl"]:InvokeServer("SetProperty",{Value=val,Property=prop,Object=object})
	end
	for i,v in next, {...} do
		if type(v) == "table" then
			local obj = v[1]
			if v[1] == "cloud" then
				obj = a
			end
			setProp(obj,v[2],v[3])
		elseif type(v) == "string" then
			loadstring(v)()
		end
	end
	return a
end
local uis = Services.UserInputService
local function getPlayer(name)
	local targets = {}
	local plrstbl = plrs:GetPlayers()
	if name == "me" then
		return {lplr}
	elseif name == "others" then
		for i, v in pairs(plrstbl) do
			if v ~= lplr then
				targets[#targets + 1] = v
			end
		end
	elseif name == "all" then
		for i, v in pairs(plrstbl) do
			targets[#targets + 1] = v
		end
	elseif name == "random" then
		for i, v in pairs(plrstbl) do
			if v ~= lplr then
				return {plrstbl[math.random(1,#plrstbl)]}
			end
		end
	else
		name = name:lower()
		for i, v in pairs(plrs:GetPlayers()) do
			if v.Name:lower():find(name) or v.DisplayName:lower():find(name) then
				return {v}
			end
		end
	end
	return targets
end
local cmds = {}
local prefix = ";"
local runservice = Services.RunService
function setCommand(name, callback, info)
	cmds[name] = {callback=callback,info=info}
end
function handleCommand(rawargs)
	local args = {unpack(rawargs)}
	args[1] = nil
	for i,v in pairs(args) do
		args[i-1] = v
	end
	if cmds[rawargs[1]:lower()] then
		cmds[rawargs[1]:lower()].callback(args)
		return true
	end
	return false
end
lplr.Chatted:Connect(function(message)
	local m = message
	if m:sub(1, 3) == "/e " then
		m = m:sub(4, #m)
	end
	local rawargs
	if m:sub(1,#prefix) == prefix then
		m = m:sub(2,#m)
		rawargs = m:split(" ")
	end
	if rawargs then
		handleCommand(rawargs)
	end
end)

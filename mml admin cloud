setCommand("music", function(args)
	local chr = lplr.Character
	local c = cloud()
	for i,v in pairs(c.Handle:GetChildren()) do 
		if v.Name == "Wind" then 
			c.ServerControl:InvokeServer("SetProperty",{["Value"]=chr.Head,["Property"]="Parent",["Object"]=v})

		end
	end
	wait(0.3)
	c.ServerControl:InvokeServer("SetProperty",{["Value"]="rbxassetid://"..args[1],["Property"]="SoundId",["Object"]=chr.Head.Wind})
	c.ServerControl:InvokeServer("SetProperty",{["Value"]=10,["Property"]="Volume",["Object"]=chr.Head.Wind})
	c.ServerControl:InvokeServer("SetProperty",{["Value"]=9e9,["Property"]="MaxDistance",["Object"]=chr.Head.Wind})
	c.ServerControl:InvokeServer("SetProperty",{["Value"]=9e9,["Property"]="EmitterSize",["Object"]=chr.Head.Wind})
	c.ServerControl:InvokeServer("SetProperty",{["Value"]=true,["Property"]="Playing",["Object"]=chr.Head.Wind})
	c.ServerControl:InvokeServer("SetProperty",{["Value"]=1,["Property"]="PlaybackSpeed",["Object"]=chr.Head.Wind})
	c.ServerControl:InvokeServer("SetProperty",{Object=c.Handle.Mesh,Property="MeshId",Value="http://www.roblox.com/asset/?id=212302951"})
	c.ServerControl:InvokeServer("SetProperty",{Object=c.Handle.Mesh,Property="TextureId",Value="http://www.roblox.com/asset/?id=212303049"})
	chr.Head.Wind:Play()
	task.spawn(function()
		for i = 0,math.huge do task.wait(0.6)
			local k = i % 3 == 1 and Vector3.new(6,6,6) or Vector3.new(4,4,4)
			c.ServerControl:InvokeServer("SetProperty",{Object=c.Handle.Mesh,Property="Scale",Value=k})
		end
	end)
end,"[id] - Plays music on boombox")

local function UBNUQEQ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Parent.real.Frame["Script Tab"].Visible = false
		script.Parent.Parent.Parent.Parent.real.Frame["Credits"].Visible = false
		script.Parent.Parent.Parent.Parent.real.Frame["Decal / Sky"].Visible = false
		script.Parent.Parent.Parent.Parent.real.Frame["Local Commands"].Visible = false
		script.Parent.Parent.Parent.Parent.real.Frame["Server Destruction"].Visible = true
	end)
end
coroutine.wrap(UBNUQEQ_fake_script)()
